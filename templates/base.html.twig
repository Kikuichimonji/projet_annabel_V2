<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Bienvenu{% endblock %}</title>
        {% block stylesheets %}
            <!-- UIkit CSS  -->
           
            <link rel="stylesheet" href="{{ asset('css/uikit.min.css') }}" />
            <link rel="stylesheet" href="{{ asset('css/style.css') }}">
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fullcalendar/core@4.1.0/main.min.css">
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fullcalendar/daygrid@4.1.0/main.min.css">
            <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fullcalendar/timegrid@4.1.0/main.min.css">
                   
            
            <style type="text/css">
            @font-face {
                font-family: 'Roboto';
                src: url({{ asset('font/Roboto-Regular.ttf') }}) format('sans-serif');
            }
            @font-face {
                font-family: 'Noto Sans JP';
                src: url({{ asset('font/NotoSansJP-Regular.otf') }}) format('sans-serif');
            }
            @font-face {
                font-family: 'Noto Sans JP Medium';
                src: url({{ asset('font/NotoSansJP-Medium.otf') }}) format('sans-serif');
            }
            </style>
        {% endblock %}
    </head>
    <body>
        <!--<main class="uk-container uk-container-large">-->
        <main>
            {% block nav %}
            {% endblock %}
            {% block body %}
            {% endblock %}
        </main>
        {% block javascripts %}

            <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
            
            <script src="//cdn.jsdelivr.net/npm/less" ></script>
            <!-- UIkit JS -->
            <script src={{ asset('js/uikit.min.js') }}></script>
            <script src={{ asset('js/uikit-icons.min.js') }}>></script>
            <script src="https://cdn.jsdelivr.net/npm/@fullcalendar/core@4.1.0/main.min.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/@fullcalendar/interaction@4.1.0/main.min.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/@fullcalendar/daygrid@4.1.0/main.min.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/@fullcalendar/timegrid@4.1.0/main.min.js"></script>
            <script rel="stylesheet" src="{{ asset('js/moment.js') }}"></script>
            <script rel="stylesheet" src="{{ asset('js/moment-timezone.js') }}"></script>
            
            
        {% endblock %}
    </body>
</html>
